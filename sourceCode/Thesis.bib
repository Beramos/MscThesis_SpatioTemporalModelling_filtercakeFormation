journal = {Colloids and Surfaces B: Biointerfaces},
keywords = {Adhesion,Bacteria,Colloid stability,DLVO theory},
month = {aug},
number = {1-4},
pages = {105--119},
pmid = {19866453},
title = {{The DLVO theory in microbial adhesion}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0927776599000296},
volume = {14},
year = {1999}
}
