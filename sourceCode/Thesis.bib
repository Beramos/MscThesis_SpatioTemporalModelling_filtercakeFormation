journal = {Journal of colloid and interface science},
keywords = {filtration,fouling,microfiltration,protein fouling},
number = {2},
pages = {389--399},
pmid = {11097775},
title = {{A Combined Pore Blockage and Cake Filtration Model for Protein Fouling during Microfiltration.}},
