pages = {022204},
pmid = {25215730},
title = {{Influence of particle size distribution on random close packing of spheres}},
url = {http://arxiv.org/abs/1303.4627 http://link.aps.org/doi/10.1103/PhysRevE.90.022204},
volume = {90},
year = {2014}
}
