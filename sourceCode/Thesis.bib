journal = {Journal of Membrane Science},
keywords = {(Bio)fouling,Cleaning,Drinking water,Membrane Fouling Simulator,Nanofiltration,Reverse osmosis},
pages = {316--324},
title = {{The Membrane Fouling Simulator: A practical tool for fouling prediction and control}},
