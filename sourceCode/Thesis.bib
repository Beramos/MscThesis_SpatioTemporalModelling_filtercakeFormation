issn = {13835866},
journal = {Separation and Purification Technology},
keywords = {Adsorption,Filtration performance,Fouling,Polyphenol,Wine filtration},
number = {3},
pages = {335--342},
title = {{Fouling in microfiltration of wine: The influence of the membrane polymer on adsorption of polyphenols and polysaccharides}},
volume = {68},
year = {2009}
}
